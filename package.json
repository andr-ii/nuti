{
  "name": "nuti",
  "version": "1.2.9",
  "author": "Andrii Lytovchenko <andr.lyt.dev@gmail.com>",
  "license": "MIT",
  "description": "A collection of commonly used utils for Node.js.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:andr-ii/nuti.git"
  },
  "files": [
    "lib",
    "docs"
  ],
  "keywords": [
    "nodejs",
    "typescript",
    "javascript",
    "library",
    "utils"
  ],
  "scripts": {
    "build": "tsc",
    "types": "tsc --noEmit",
    "format": "prettier --write '**/*.{js,json,md,ts,yml,yaml}'",
    "format:check": "prettier -l '**/*.{js,json,md,ts,yml,yaml}'",
    "lint": "eslint --fix '**/*.ts'",
    "lint:check": "eslint '**/*.ts'",
    "spell": "cspell '**/*'",
    "hooks": "npsh.hooks",
    "svc": "npsh.svc andr-ii nuti master"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cspell": "^6.8.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "npsh": "^1.0.6"
  }
}
