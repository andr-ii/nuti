{
  "name": "nuti",
  "version": "2.0.0",
  "author": "Andrii Lytovchenko <andr.lyt.dev@gmail.com>",
  "license": "MIT",
  "description": "A collection of commonly used utils for Node.js.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:andr-ii/nuti.git"
  },
  "files": [
    "lib",
    "docs"
  ],
  "contributors": [
    {
      "name": "Andrii Lytovchenko",
      "email": "andr.lyt.dev@gmail.com",
      "url": "https://github.com/andr-ii"
    }
  ],
  "keywords": [
    "nodejs",
    "typescript",
    "javascript",
    "library",
    "utils"
  ],
  "scripts": {
    "build": "tsc",
    "types": "tsc --noEmit",
    "format": "prettier --write '**/*.{js,json,md,ts,yml,yaml}'",
    "format:check": "prettier -l '**/*.{js,json,md,ts,yml,yaml}'",
    "lint": "eslint --fix '**/*.ts'",
    "lint:check": "eslint '**/*.ts'",
    "spell": "cspell '**/*'",
    "hooks": "npsh hooks",
    "svc": "npsh svc andr-ii nuti master",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "cspell": "^6.31.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "npsh": "^2.0.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.7.0",
    "typescript": "^5.0.2"
  }
}
