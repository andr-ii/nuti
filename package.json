{
  "name": "nuti",
  "version": "1.5.4",
  "author": "Andrii Lytovchenko <andr.lyt.dev@gmail.com>",
  "license": "MIT",
  "description": "A collection of commonly used utils for Node.js.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:andr-ii/nuti.git"
  },
  "files": [
    "lib",
    "docs"
  ],
  "keywords": [
    "nodejs",
    "typescript",
    "javascript",
    "library",
    "utils"
  ],
  "scripts": {
    "build": "tsc",
    "types": "tsc --noEmit",
    "format": "prettier --write '**/*.{js,json,md,ts,yml,yaml}'",
    "format:check": "prettier -l '**/*.{js,json,md,ts,yml,yaml}'",
    "lint": "eslint --fix '**/*.ts'",
    "lint:check": "eslint '**/*.ts'",
    "spell": "cspell '**/*'",
    "hooks": "npsh hooks",
    "svc": "npsh svc andr-ii nuti master",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cspell": "^6.19.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.3.1",
    "npsh": "^2.0.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.9.2"
  }
}
